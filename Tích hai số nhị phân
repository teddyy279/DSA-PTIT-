#include<bits/stdc++.h>

using namespace std;
using ll = long long;

int mod = 1e9 + 7;

int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int t; cin >> t;
    while(t--){
        string a, b; cin >> a >> b;
        int decimal_a = stoi(a, nullptr, 2);
        int decimal_b = stoi(b, nullptr, 2);
        cout << decimal_a * decimal_b << endl;
    }
}
